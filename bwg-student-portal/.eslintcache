         case \"auth/invalid-email\":\r\n            setEmailError(err.message);\r\n            break;\r\n          case \"auth/weak-password\":\r\n            setPasswordError(err.message);\r\n            break;\r\n        }\r\n      });\r\n  };\r\n\r\n  const handleLogout = () => {\r\n    fire.auth().signOut();\r\n  };\r\n\r\n  const authListener = () => {\r\n    fire.auth().onAuthStateChanged((user) => {\r\n      if (user) {\r\n        // clearInputs();\r\n        setUser(user);\r\n        setEmail(user.email)\r\n\r\n        fetch(\"/username/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              setName(data.firstName)\r\n            } else {\r\n              setUser(\"\")\r\n            }\r\n          });\r\n        });\r\n        //total classes attended\r\n        fetch(\"/hourstrained/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setHoursTrained(data)\r\n            } else {\r\n              setUser(user)\r\n              setHoursTrained(0)\r\n            }\r\n          });\r\n        });\r\n        //total classes attended in the last 7 days\r\n        fetch(\"/sevendays/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setSevenDays(data)\r\n            } else {\r\n              setUser(user)\r\n              setSevenDays(0)\r\n            }\r\n          });\r\n        });\r\n        //total bodyweight classes attended\r\n        \r\n        fetch(\"/bodyweightClass/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setBodyweightClass(data)\r\n            } else {\r\n              setUser(user)\r\n              setBegClass(0)\r\n            }\r\n          });\r\n        });\r\n\r\n\r\n        // total mobility classes attended \r\n        fetch(\"/mobilityClass/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setMobilityClass(data)\r\n            } else {\r\n              setUser(user)\r\n              setMobilityClass(0)\r\n            }\r\n          });\r\n        });\r\n\r\n        // total beginner bodyweight classes attended \r\n        fetch(\"/begClass/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setBegClass(data)\r\n            } else {\r\n              setUser(user)\r\n              setBegClass(0)\r\n            }\r\n          });\r\n        });\r\n\r\n        //total cali classes attended \r\n        fetch(\"/caliClass/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setCaliClass(data)\r\n            } else {\r\n              setUser(user)\r\n              setCaliClass(0)\r\n            }\r\n          });\r\n        });\r\n\r\n        //total handstand classes attended \r\n        fetch(\"/handstandClass/\" + user.email).then((res) => {\r\n          res.json().then((data) => {\r\n            if (data) {\r\n              // clearInputs()\r\n              setHandstandClass(data)\r\n            } else {\r\n              setUser(user)\r\n              setHandstandClass(0)\r\n            }\r\n          });\r\n        });\r\n\r\n      } else {\r\n        setUser(\"\");\r\n      }\r\n    });\r\n  };\r\n\r\n\r\n  useEffect(() => {\r\n    authListener();\r\n  }, []);\r\n\r\n  //login screen\r\n  return (\r\n    <div className=\"App\">\r\n      {user ? ( //if user exists render document 'hero' replace with dash board\r\n        <Dashboard\r\n          handleLogout={handleLogout}\r\n          name={name}\r\n          email={email}\r\n          hou